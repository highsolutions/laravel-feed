{
    "name": "highsolutions/feed",
    "description": "A RSS2 feed generator for Laravel.",
    "keywords": ["laravel", "feed", "generator", "rss", "atom"],
    "license": "MIT",
    "authors": [
        {
            "name": "Roumen Damianoff",
            "email": "roumen@crimsson.com",
            "role": "Developer",
            "homepage": "https://roumen.it"
        },
        {
            "name": "HighSolutions",
            "email": "adam@highsolutions.pl"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "orchestra/testbench": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "HighSolutions\\Feed\\": "src/"
        }
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 5.5"],
        "laravel": {
            "providers": [
                "HighSolutions\\Feed\\FeedServiceProvider"
            ],
            "aliases": {
                "Feed": "HighSolutions\\Feed\\Feed"
            }
        }
    },
    "minimum-stability": "stable"
}
